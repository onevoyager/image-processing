<%= form_for @image, url: images_path, html: { multipart: true } do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
  
    <%= f.label :description %>
    <%= f.text_area :description %>
  
    <%= f.label :author %>
    <%= f.text_field :author %>
  
    <%= f.label :image_file %>
    <%= f.file_field :image_file %>
  
    <%= f.submit %>
<% end %>
  





































